//Calcular el producto de los primeros N n√∫meros enteros.
#include "stdafx.h"
#include <iostream>
#include <conio.h>
using namespace std;
void main() {
	int i,s,n;
	cout << "Ingrese un numero entero positivo: " ;
	cin>>n;
	s=1;
	for(i=1;i<=n;i++){
		s=s*(i);
	}
	cout<<"El producto de los numeros hasta el " << n << " seria: " << s;
	getch();}
